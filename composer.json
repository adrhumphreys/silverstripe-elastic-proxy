{
    "name": "madmatt/silverstripe-elastic-proxy",
    "description": "A simple proxy module to ensure your Elastic API keys aren't leaked.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",

    "require": {
        "silverstripe/framework": "^4.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },

    "autoload": {
        "psr-4": {
            "Madmatt\\ElasticProxy\\": "src/"
        }
    },

    "keywords": [
        "silverstripe",
        "CMS",
        "elastic",
        "search",
        "elasticsearch",
        "elastic-app-search"
    ]
}
